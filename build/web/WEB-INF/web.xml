<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>SwitchServlet</servlet-name>
        <servlet-class>com.servlets.SwitchServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AutoServlet</servlet-name>
        <servlet-class>com.servlets.AutoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CurrentServlet</servlet-name>
        <servlet-class>com.servlets.CurrentServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Control_M_Servlet</servlet-name>
        <servlet-class>com.servlets.Control_M_Servlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Control_M_Servlet</servlet-name>
        <url-pattern>/Control_M_Servlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CurrentServlet</servlet-name>
        <url-pattern>/CurrentServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SwitchServlet</servlet-name>
        <url-pattern>/SwitchServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AutoServlet</servlet-name>
        <url-pattern>/AutoServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
